void setup()   {                
  Serial.begin(9600);
  //while (! Serial) delay(100);
  
  if ( ! display.begin(0x3c) ) {
     Serial.println("Unable to initialize OLED");
     while (1) yield();
  }
  Serial.println();
  Serial.println();
  Serial.print("Establishing Connection To ");
  Serial.print(ssid);
  // Had compatibility issues without, you may not need it.
  client.setInsecure();
  // Connect to WiFi
  WiFi.begin(ssid, password);
   if ( ! display.begin(0x3C) ) {
     Serial.println("Unable to initialize OLED");
     while (1) yield();
  }
  timeClient.begin();
  display.clearDisplay();
  display.drawBitmap(0, 0,  subcountbootupbackground , 128, 90, 8);
  display.setContrast(100);
  display.display();
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  ArduinoOTA.setPassword((const char *)"123");
  ArduinoOTA.onStart([]() {
    Serial.println("Start");
  });
  ArduinoOTA.onEnd([]() {
    Serial.println("\nEnd");
  });
  ArduinoOTA.onProgress([](unsigned int progress, unsigned int total) {
    Serial.printf("Progress: %u%%\r", (progress / (total / 100)));
  });
  ArduinoOTA.onError([](ota_error_t error) {
    Serial.printf("Error[%u]: ", error);
    if (error == OTA_AUTH_ERROR) Serial.println("Auth Failed");
    else if (error == OTA_BEGIN_ERROR) Serial.println("Begin Failed");
    else if (error == OTA_CONNECT_ERROR) Serial.println("Connect Failed");
    else if (error == OTA_RECEIVE_ERROR) Serial.println("Receive Failed");
    else if (error == OTA_END_ERROR) Serial.println("End Failed");
  });
  ArduinoOTA.begin();
  Serial.println("");
  Serial.println("WiFi connected");
  Serial.println("IP address: ");
  IPAddress ip = WiFi.localIP();
  Serial.println(ip);

  display.clearDisplay();
  display.drawBitmap(0, 0, smileymacbackground, 128, 90, 8);
  display.display();
  delay(3000);
  display.clearDisplay();
  display.drawBitmap(1,1, welcometosubosbackground, 128, 90, 8);
  display.display();
  delay(3000);
  

}

int loopCount = 4;
String currentTime = "";
int screensavercount = 0;
void loop() {
  timeClient.update();
  if (timeClient.getHours() > 12) {
    currentTime = String(timeClient.getHours() - 12);
  }
  else{
    currentTime = String(timeClient.getHours());
  }
  currentTime = currentTime + ":";
  if (timeClient.getMinutes() < 10){
    currentTime = currentTime + "0" + String(timeClient.getMinutes());
  }
  else {
    currentTime = currentTime + String(timeClient.getMinutes());
  }
  if (loopCount == 4) {
    api2.getChannelStatistics(CHANNEL_ID2);
    api.getChannelStatistics(CHANNEL_ID);
    loopCount = 0;
  }
  display.setRotation(0);
  
  // miniature bitmap display
  display.clearDisplay();
  display.drawBitmap(0, 0,  SubCountBackground, 128, 90,8);
  display.setContrast(100);
  display.setTextSize(1);
  display.setTextColor(SSD1327_BLACK);
  display.setCursor(92, 4);
  display.print(currentTime);

  
  display.setTextSize(1);
  display.setTextColor(SSD1327_BLACK);

 // On loop 0 and loop 2, let's display the first channel information
  if (loopCount == 0 || loopCount == 2) {
    display.setCursor(8, 39);
    display.println(channel1nameline1);
    display.setCursor(8, 49);
    display.println(channel1nameline2);
    display.setCursor(33,63);
    display.println(String(api.channelStats.subscriberCount));
    display.setCursor(38, 74);
    display.println(String(api.channelStats.viewCount));     
  }
// Otherwise display the secondary channel info. 
  else 
  {
    display.setCursor(8, 39);
    display.println(channel2nameline1);
    display.setCursor(8, 49);
    display.println(channel2nameline2);
    display.setCursor(33,63);
    display.println(String(api2.channelStats.subscriberCount));
    display.setCursor(38, 74);
    display.println(String(api2.channelStats.viewCount));
  }

  display.display();

  delay(5000);
  loopCount += 1;
  screensavercount += 1;
  // When the screensaver loop count hits 2, run the ScreenSaver subroutine
  if (screensavercount == 2){
    ScreenSaver();
  }
  // If we get to 4, dispaly the original Macintosh "Hello" screen, and reset to 0 for the screensaver
  if (screensavercount >= 4) {
    display.clearDisplay();
    display.drawBitmap(0, 0,  macminihello, 128, 90,8);
    display.setContrast(100);
    display.display();
    delay(5000);
    screensavercount = 0;
  }
}

// Toaster screensaver
void ScreenSaver() {
  display.clearDisplay();
  display.setCursor(0, 40);
  display.setTextSize(1);
  display.setTextColor(SSD1327_WHITE);
  display.println("Is it after dark yet?");
  display.display();
  delay(2000);
  int x = 128;
  int y = 1;
  int frame = 0;
  while (x > -34) {
     display.clearDisplay();
     if (frame == 0) {
      display.drawBitmap(x, y, toaster0, 32, 32, 8);
     }
     if (frame == 1) {
      display.drawBitmap(x, y, toaster1, 32, 32, 8);
     }
     if (frame == 2) {
      display.drawBitmap(x, y, toaster2, 32, 32, 8);
     }
     if (frame == 3) {
      display.drawBitmap(x, y, toaster1, 32, 32, 8);
     }
     display.setContrast(100);
     display.display();
     delay(10);
     frame = frame + 1;
     if (frame == 4) {
      frame = 0;
    }
    x = x - 3;
    y = y + 1;
  }

}
